<% @groups.each do |group| %>
  <div class="group">
    <div class="group_name">
      <%= group.name %>
    </div>
    <div class="group_members">
      <% group.members.each do |member| %>
        <div>
          <%= member.name %> : <%= member.year %>
        </div>
      <% end %>
    </div>
  </div>
<% end %>